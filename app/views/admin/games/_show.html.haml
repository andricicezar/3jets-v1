- fst_user = User.find(game.first_user.user_id)
- scd_user = User.find(game.second_user.user_id)
%h2
  - if game.winner == fst_user.id
    %span.winner
      = fst_user.email_splited
  - else
    %span
      = fst_user.email_splited
  vs
  - if game.winner == scd_user.id
    %span.winner
      = scd_user.email_splited
  - else
    %span
      = scd_user.email_splited
